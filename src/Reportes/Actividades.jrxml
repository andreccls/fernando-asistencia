	<parameter name="DNI" class="java.lang.String"/>
	<parameter name="Dia" class="java.lang.String"/>
	<parameter name="Diafin" class="java.lang.String"/>
	<parameter name="Mes" class="java.lang.String"/>
	<parameter name="Ano" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     tarea.`Nombre` AS tarea_Nombre,
     ano.`Ano` AS ano_Ano,
     mes.`Mes` AS mes_Mes,
     dia.`Dia` AS dia_Dia,
     iniciofin.`Inicio` AS iniciofin_Inicio,
     iniciofin.`Fin` AS iniciofin_Fin,
     personal.`DNI` AS personal_DNI,
     tarea.`Estado` AS tarea_Estado
FROM
     `tarea` tarea INNER JOIN `agenda` agenda ON tarea.`Id_tarea` = agenda.`Id_tarea`
     INNER JOIN `ano` ano ON agenda.`Id_personal` = ano.`Id_personal`
     AND agenda.`Id_tarea` = ano.`Id_tarea`
     INNER JOIN `personal` personal ON agenda.`Id_personal` = personal.`Id_personal`
     INNER JOIN `mes` mes ON ano.`Id_ano` = mes.`Id_ano`
     INNER JOIN `dia` dia ON mes.`Id_mes` = dia.`Id_mes`
     INNER JOIN `iniciofin` iniciofin ON dia.`Id_dia` = iniciofin.`Id_dia`
WHERE
	DNI = $P{DNI}
 AND tarea.Estado=1
 AND Ano = $P{Ano}
 AND Mes = $P{Mes}
 AND Dia between $P{Dia} and $P{Diafin}
order by Dia]]>
	</queryString>
