     `personal` personal INNER JOIN `agenda` agenda ON personal.`Id_personal` = agenda.`Id_personal`
     INNER JOIN `tarea` tarea ON agenda.`Id_tarea` = tarea.`Id_tarea`
     INNER JOIN `ano` ano ON agenda.`Id_personal` = ano.`Id_personal`
