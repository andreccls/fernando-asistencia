	<parameter name="Estado" class="java.lang.String"/>
	<parameter name="Tardanza" class="java.lang.String"/>
	<parameter name="Mes" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     tarea.`Nombre` AS tarea_Nombre,
     personal.`Apellido` AS personal_Apellido,
     personal.`Nombre` AS personal_Nombre,
     ano.`Ano` AS ano_Ano,
     mes.`Mes` AS mes_Mes,
     dia.`Dia` AS dia_Dia,
     iniciofin.`Inicio` AS iniciofin_Inicio,
     iniciofin.`Fin` AS iniciofin_Fin,
     asistencia.`Estado` AS asistencia_Estado,
     asistencia.`Tardanza` AS asistencia_Tardanza,
     justificacion.`Motivo` AS justificacion_Motivo,
     articulo.`Nombre` AS articulo_Nombre,
     articulo.`Nro_articulo` AS articulo_Nro_articulo
FROM
     `tarea` tarea INNER JOIN `agenda` agenda ON tarea.`Id_tarea` = agenda.`Id_tarea`
     INNER JOIN `personal` personal ON agenda.`Id_personal` = personal.`Id_personal`
     INNER JOIN `ano` ano ON agenda.`Id_personal` = ano.`Id_personal`
     AND agenda.`Id_tarea` = ano.`Id_tarea`
     INNER JOIN `mes` mes ON ano.`Id_ano` = mes.`Id_ano`
     INNER JOIN `dia` dia ON mes.`Id_mes` = dia.`Id_mes`
     INNER JOIN `iniciofin` iniciofin ON dia.`Id_dia` = iniciofin.`Id_dia`
     INNER JOIN `asistencia` asistencia ON iniciofin.`Id_iniciofin` = asistencia.`Id_iniciofin`
     INNER JOIN `justificacion` justificacion ON asistencia.`Id_asistencia` = justificacion.`Id_asistencia`
     INNER JOIN `articulo` articulo ON justificacion.`Id_articulo` = articulo.`Id_articulo`
WHERE
     asistencia.Estado = $P{Estado}
 AND asistencia.Tardanza = $P{Tardanza}
 AND mes=$P{Mes}]]>
	</queryString>
