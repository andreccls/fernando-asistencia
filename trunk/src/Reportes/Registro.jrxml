	<queryString>
		<![CDATA[SELECT
     registroacceso.`Inicio` AS registroacceso_Inicio,
     registroacceso.`Fin` AS registroacceso_Fin,
     registroacceso.`Fecha` AS registroacceso_Fecha,
     personal.`DNI` AS personal_DNI
FROM
     `personal` personal INNER JOIN `registroacceso` registroacceso ON personal.`Id_personal` = registroacceso.`Id_personal`
WHERE
	DNI= $P{DNI}
 AND Year(Fecha) = $P{Ano}
 AND month(Fecha) = $P{Mes}
 AND day(Fecha) between $P{Dia} and $P{Diafin}
ORDER BY
     Fecha ASC]]>
